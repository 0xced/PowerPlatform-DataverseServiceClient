<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="ConnectControlForegroundBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
	<SolidColorBrush x:Key="ConnectControlCheckBoxForegroundBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
	<SolidColorBrush x:Key="ConnectControlBackgroundBrush" Color="{x:Static SystemColors.WindowColor}"/>
	<SolidColorBrush x:Key="ConnectControlErrorForegroundColor" Color="{x:Static SystemColors.WindowTextColor}"/>
	<SolidColorBrush x:Key="ConnectControlOnSelectedListViewBackgroundBrush" Color="{x:Static SystemColors.HighlightColor}"/>
	<SolidColorBrush x:Key="ConnectControlSortOrderBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
</ResourceDictionary>