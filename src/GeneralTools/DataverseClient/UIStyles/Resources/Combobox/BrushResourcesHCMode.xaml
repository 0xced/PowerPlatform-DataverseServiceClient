<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ComboboxBackgroundBrush" Color="{x:Static SystemColors.WindowColor}"/>
    <SolidColorBrush x:Key="ComboboxchkBoxBorderBackgroundBrush" Color="{x:Static SystemColors.WindowColor}"/>
    <SolidColorBrush x:Key="ComboboxchkBoxBorderDisabledBackgroundBrush" Color="{x:Static SystemColors.WindowColor}"/>
    <SolidColorBrush x:Key="ComboboxchkBoxBorderDisabledBorderBrush" Color="{x:Static SystemColors.WindowColor}"/>
    <SolidColorBrush x:Key="ComboboxchkBox2BorderBackgroundBrush" Color="{x:Static SystemColors.WindowColor}"/>
    <SolidColorBrush x:Key="ComboboxchkBoxForegroundBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
    <SolidColorBrush x:Key="ComboboxForegroundBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
    <SolidColorBrush x:Key="ComboboxToggeleButtonPathArrowFillBrush" Color="{x:Static SystemColors.WindowTextColor}"/>
    <SolidColorBrush x:Key="ComboboxOnOverBackgroundBrush" Color="{x:Static SystemColors.HighlightColor}"/>
    <SolidColorBrush x:Key="ComboboxOnOverForegroundBrush" Color="{x:Static SystemColors.HighlightTextColor}"/>
    <SolidColorBrush x:Key="ComboboxDisabledForegroundBrush" Color="{x:Static SystemColors.GrayTextColor}"/>
</ResourceDictionary>